<script type="text/javascript">
    $(function() {
        $(window).scroll(function() {
            if($(this).scrollTop() != -20) {
                $('#topNubex').fadeIn();
            } else {
                $('#topNubex').fadeOut();
            }
        });
        $('#topNubex').click(function() {
            $('body,html').animate({scrollTop:0},700);
        });
    });
</script>

